{
   "content_scripts":
   [
      {
         "all_frames": true,
         "js": ["JQuery1.9.1.js", "OnPageLoad.js"],
         "matches": ["http://www.quora.com/*", "https://www.quora.com/*"],
         "run_at": "document_start"
      }
   ],
   "description": "Adds answer-sorting and other features to Quora.",
   "icons": {"128": "Logo_128.png"},
   "name": "VQuora",
   "permissions": ["tabs", "http://*/", "https://*/"],
   "version": "1.0.5",
   "manifest_version": 2
}